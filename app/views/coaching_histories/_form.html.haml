= form_for @coaching_history do |f|
  - if @coaching_history.errors.any?
    #error_explanation
      %h2= "#{pluralize(@coaching_history.errors.count, "error")} prohibited this coaching_history from being saved:"
      %ul
        - @coaching_history.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :coach_id
    = f.number_field :coach_id
  .field
    = f.label :team_id
    = f.number_field :team_id
  .field
    = f.label :year
    = f.text_field :year
  .field
    = f.label :role
    = f.text_field :role
  .actions
    = f.submit 'Save'
